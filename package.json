{
    "name": "timings-client-js",
    "version": "0.0.0-development",
    "description": "Plugin for perf-api to support JavaScript based test environments",
    "main": "index.js",
    "scripts": {
        "lint": "npx eslint .",
        "test": "nyc --reporter=cobertura npm run test:mocha",
        "test:mocha": "mocha test/**/*.js --exit",
        "semantic-release": "npx semantic-release --no-ci",
        "semantic-release:dry-run": "npx semantic-release --dry-run --no-ci --branches `git rev-parse --abbrev-ref HEAD`",
        "prepare": "git config core.hooksPath .git/hooks/ && npx simple-git-hooks"

    },
    "repository": {
        "type": "git",
        "url": "https://github.com/godaddy/timings-client-js.git"
    },
    "bugs": {
        "url": "https://github.com/godaddy/timings-client-js/issues"
    },
    "keywords": [
        "timings",
        "helper",
        "performance",
        "kibana",
        "elasticsearch",
        "perftiming"
    ],
    "homepage": "https://github.com/godaddy/timings-client-js",
    "author": "GoDaddy Operating Company, LLC",
    "contributors": [
        "Marcel Verkerk <mverkerk@godaddy.com>",
        "Gergely Nemeth <gnemeth@godaddy.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "axios": "^1.4.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "async": "^3.2.4",
        "chai": "^4.3.7",
        "eslint": "^8.41.0",
        "eslint-config-godaddy": "^7.0.2",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-mocha": "^10.1.0",
        "mocha": "^10.2.0",
        "nock": "^13.3.1",
        "nyc": "^15.1.0",
        "semantic-release": "^21.0.2",
        "simple-git-hooks": "^2.8.1"
    }
}
